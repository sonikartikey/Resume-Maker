<h2>fill your educations details</h2>
<div class ="center">
<%= form_for @education , method: :patch do |f| %>
  <div class="form-group">
    <label for="exampleInputEmail1">University Name</label>
    <%= f.text_field :university_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Stream</label>
    <%= f.text_field :stream,class: "form-control", placeholder:"Enter Email" %>
  </div>
    <div class="form-group">
    <label for="exampleInputPassword1">Percentage</label>
    <%= f.text_area :percentage,class: "form-control" %>
  </div>
      <div class="form-group">
    <label for="exampleInputPassword1">qualification</label>
    <%= f.text_area :qualification,class: "form-control" %>
  </div>
    <div class="form-group">
    <label for="exampleInputPassword1">Description</label>
    <%= f.text_area :start_date,class: "form-control"%>
  </div>
      <div class="form-group">
    <label for="exampleInputPassword1">Description</label>
    <%= f.text_area :end_date,class: "form-control"%>
  </div>
  <%=f.hidden_field :user_id, value: current_user.id%>
  <%=f.submit  %>
<% end %>
</div>
<%=link_to 'Back', educations_path %>