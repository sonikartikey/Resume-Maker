<h2>fill your skill details</h2>
<div class ="center">
<%= form_for @skill do |f| %>
  <div class="form-group">
    <label for="exampleInputEmail1">languages Known</label>
    <%= f.text_field :languages, class: "form-control" , id: "exampleInputEmail1" , placeholder: "Enter university_name" %>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Description</label>
    <%= f.text_field :description,class: "form-control", placeholder:"stream" %>
</div>
  <%=f.hidden_field :user_id, value: current_user.id%>
  <%=f.submit  %>
<% end %>
</div>

<%=link_to 'Add More Education' , add_more_path %>

